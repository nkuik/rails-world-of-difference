<div class="container">
  <div class="row">
    <% @projects.each do |project|%>
      <div class="col-md-6 col-xs-12">
        <div class="card">
          <%= link_to project_path(project) do%>
          <h3><%= project.name %></h3>
          <h5>Skills</h5>
          <p><%= project.description %></p>
            <% project.project_skills.each do |project_skill| %>
              <h3> <%= project_skill.skill.name %></h3>
              <%= project_skill.level%>
            <% end %>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
</div>
